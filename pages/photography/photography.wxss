.photographyWrpper-title {
  border-left: 4px solid #5ddcdc;
  margin: 1rem 0;
  margin-left: 0.5rem;
  padding-left: 0.5rem;
}

.photographyWrpper-content {
  display: grid;
  grid-template-columns: 30% 30% 30%;
  grid-column-gap: 10px;
  grid-row-gap: 10px;
  justify-content: center;
}

.photographyWrpper-content_items {
  position: relative;
  height: 100px;
  background-color: #ccc;
}

.photographyWrpper-content_image {
  width: 100%;
  height: 100%;
}
.photographyWrpper-content_text {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  color: #fff;
}
