/**app.wxss**/

.photography, .essay, .personal, .essay-detail ,.photography-detail{
  font-size: 12px;
  padding: 14rpx 14rpx;
  display: grid;
}

.bottomText {
  text-align: center;
  margin-top: 1rem;
  font-size: 10px;
  color: #999;
  margin-bottom: 20rpx;
}

.bottomText-split{
  display: inline-block;
  width: 30%;
  height: 1rpx;
  vertical-align: middle;
  background-color: #ccc;
}
/*底部按钮*/
.bottomBtn{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #fff;
  display: grid;
  grid-template-columns: 1fr 1fr;
  text-align: center;
  border-top: 1rpx solid #d9d9d9;
  height: 80rpx;
  line-height: 80rpx;
  margin: 0 -20rpx;
}
.bottomBtn text{
   border-right: 1rpx solid #d9d9d9;
}
.bottomBtn text:last-child{
  border-right: none;
}

